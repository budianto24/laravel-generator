{
    "name": "budianto24/laravel-generator",
    "type": "library",
    "description": "Laravel Generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Budianto",
            "email": "budiianto24@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^8.0",
        "laracasts/flash": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.3.1"
    },
    "autoload": {
        "psr-4": {
            "InfyOm\\Generator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\InfyOm\\Generator\\InfyOmGeneratorServiceProvider"
            ]
        }
    }    
}
